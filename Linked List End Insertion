/*
class Node {
  public:
    int data;
    Node* next;

    // Default constructor
    Node() {
        data = 0;
        next = NULL;
    }

    // Parameterised Constructor
    Node(int data) {
        this->data = data;
        this->next = NULL;
    }
};
*/
class Solution {
  public:
    Node *insertAtEnd(Node *head, int x) {
        // Code here
        Node *t=new Node(x);
        Node *temp=head;
        if(head==nullptr)
        return t;
        while(temp->next!=nullptr)
        temp=temp->next;
        temp->next=t;
        return head;
    }
};
