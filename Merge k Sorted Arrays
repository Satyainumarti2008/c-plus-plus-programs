class Solution {
  public:
    vector<int> mergeArrays(vector<vector<int>> &mat) {
        // Code here
        vector<int>t;
        for(int i=0;i<mat.size();i++){
            for(int j=0;j<mat[i].size();j++){
                t.push_back(mat[i][j]);
            }
        }
        sort(t.begin(),t.end());
        return t;
    }
};
